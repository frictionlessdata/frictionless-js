{
  "name": "frictionless.js",
  "version": "0.13.4",
  "description": "",
  "main": "dist/node/index.js",
  "directories": {
    "test": "src/test"
  },
  "scripts": {
    "test": "yarn run test:node && yarn run test:browser",
    "test:browser": "yarn build:browser && karma start karma.conf.js --browsers=ChromeHeadless",
    "test:node": "nyc mocha --require @babel/register test/*",
    "build:browser": "webpack --mode production",
    "build:browser:watch": "webpack --watch",
    "build:node": "rm -rf ./dist/node && babel ./src/ -d ./dist/node --no-comments",
    "build": "yarn run build:node && yarn run build:browser",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls && nyc report --reporter=lcov",
    "lint": "eslint ./src",
    "docs": "jsdoc2md src/**.js > docs/API.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frictionlessdata/frictionless-js.git"
  },
  "author": "datopian",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frictionlessdata/frictionless-js/issues"
  },
  "homepage": "https://github.com/frictionlessdata/frictionless-js#readme",
  "dependencies": {
    "chardet": "^1.3.0",
    "csv-parse": "^4.15.3",
    "csv-sniffer": "^0.1.1",
    "iconv-lite": "^0.6.2",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.29",
    "node-fetch": "^2.6.1",
    "readable-web-to-node-stream": "^3.0.1",
    "stream-to-array": "^2.3.0",
    "stream-to-string": "^1.2.0",
    "tableschema": "^1.12.4",
    "url-join": "^4.0.1",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-modules-umd": "^7.10.4",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.1",
    "ava": "3.13.0",
    "babel-loader": "^8.1.0",
    "browser-env": "3.3.0",
    "browserify": "16.5.2",
    "chai": "4.2.0",
    "core-js": "3.6.5",
    "coveralls": "3.1.0",
    "eslint": "7.10.0",
    "jasmine": "3.6.1",
    "jsdoc-to-markdown": "^6.0.1",
    "karma": "5.2.3",
    "karma-browserify": "7.0.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "4.0.1",
    "mocha": "8.1.3",
    "mocha-lcov-reporter": "1.3.0",
    "mocha-loader": "5.1.2",
    "mocha-webpack": "1.1.0",
    "nock": "13.0.4",
    "node-forge": "^1.3.0",
    "nyc": "15.1.0",
    "parcel-bundler": "^1.12.4",
    "source-map-loader": "1.1.0",
    "transform-loader": "0.2.4",
    "watchify": "3.11.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "xo": "0.33.1"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
